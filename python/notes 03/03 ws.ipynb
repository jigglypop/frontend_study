    "def P(s):\n",
    "\n",
    "    Q = []\n",
    "    S = []\n",
    "\n",
    "    for x in s:\n",
    "        if x.isalpha():\n",
    "            Q.append(x.lower())\n",
    "            S.append(x.lower())\n",
    "    while Q:\n",
    "        if Q.pop(0) != S.pop():\n",
    "            return False        \n",
    "    return True\n",
    "\n",
    "n = input()\n",
    "print(P(n))"
   ]
  }
