{
  "compilerOptions": {
    "target": "esnext",       
    "module": "esnext", 
    "jsx": "preserve", 
    "strict": true, 
    "noImplicitAny": true,      
    "strictNullChecks": true,             
    "moduleResolution": "node",           
    "baseUrl": "./",                       
    "paths":{"*":["node_modules/*"]},
    "esModuleInterop": true               
  },
  "exclude": [
    "**/*.test.*",
    "**/*.spec.*",
    "node_modules"
  ]
}
